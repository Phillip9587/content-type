{
  "name": "content-type",
  "description": "Create and parse HTTP Content-Type header",
  "version": "1.0.5",
  "author": "Douglas Christopher Wilson <doug@somethingdoug.com>",
  "license": "MIT",
  "keywords": [
    "content-type",
    "http",
    "req",
    "res",
    "rfc7231"
  ],
  "repository": "jshttp/content-type",
  "main": "index.cjs",
  "module": "index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@eslint/markdown": "^6.2.1",
    "c8": "^10.1.2",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "rollup": "^4.24.0"
  },
  "files": [
    "LICENSE",
    "HISTORY.md",
    "README.md",
    "index.cjs",
    "index.mjs"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "test": "node --test --test-reporter spec",
    "test-ci": "c8 --reporter=lcovonly --reporter=text npm test",
    "test-cov": "c8 --reporter=html --reporter=text npm test",
    "version": "node scripts/version-history.mjs && git add HISTORY.md"
  }
}
